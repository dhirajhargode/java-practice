	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="motor" class="com.blc.beans.Motor" init-method="start" destroy-method="destroy">
		<property name="motorId" value="101"/>
		<property name="type" value="Electic"/>
		
	</bean>
