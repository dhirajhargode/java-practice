<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
	<class name="com.sarang.domain.Domain" table="Employee">
		<id name="id" column="EID">
			<generator class="increment"/>
			</id>
			<property name="fname" column="ENAME" />
			<property name="lname" column="LNAME" />
			<property name="mail" column="MAIL" />
	</class>

</hibernate-mapping>